{
	"List SOQL Query": {
		"prefix": "listq",
		"body": "List<${1:Object}> ${2:listName} = [\n\tSELECT Id, ${3:fields}\n\tFROM ${1:Object}$4\n];$0"
	},

	"Query Single Object": {
		"prefix": "quer",
		"body": "${1:Object} ${2:varName} = [\n\tSELECT Id, ${3:fields} \n\tFROM ${1:Object} \n\tWHERE ${4:filter} = ${5:value} \n\tLIMIT 1\n];$0"
	}
}
